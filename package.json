    "pretty-quick": "^1.11.0",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "tslint-react-hooks": "^2.1.0",
    "typescript": "^3.5.1"
